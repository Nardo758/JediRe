# JediRe Supply Agent Configuration

# Database connection (use Replit's DATABASE_URL)
DATABASE_URL=${DATABASE_URL}

# Markets to analyze (semicolon-separated)
MARKETS=Austin, TX;Denver, CO;Phoenix, AZ;Miami, FL;Seattle, WA

# Agent run frequency (minutes)
AGENT_RUN_INTERVAL_MINUTES=60

# Data collection mode
USE_MOCK_DATA=true
ENABLE_ZILLOW=false
ENABLE_REDFIN=false

# AI insights (requires CLAUDE_API_KEY)
ENABLE_AI_INSIGHTS=false
CLAUDE_API_KEY=

# Logging
LOG_LEVEL=INFO
