{
  "name": "jedire",
  "version": "1.0.0",
  "description": "JEDI RE - Real Estate Intelligence Platform",
  "private": true,
  "scripts": {
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "start": "cd backend && npm run start",
    "start:prod": "cd backend && npm run start:prod",
    "migrate": "cd backend && npm run migrate",
    "deploy:check": "npm run build && echo 'âœ… Deploy check passed!'",
    "test": "echo 'Running tests...' && cd backend && npm test 2>/dev/null || echo 'No tests configured yet'",
    "lint": "cd frontend && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/jedire.git"
  },
  "keywords": [
    "real-estate",
    "intelligence",
    "multifamily",
    "commercial-real-estate"
  ],
  "author": "Your Name",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@turf/area": "^7.3.4",
    "@types/multer": "^2.0.0",
    "@types/three": "^0.182.0",
    "google-auth-library": "^10.5.0",
    "googleapis": "^171.4.0",
    "multer": "^2.0.2",
    "puppeteer": "^24.36.1",
    "three": "^0.182.0"
  }
}
